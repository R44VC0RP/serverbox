DAYTONA_API_KEY=your_daytona_api_key

# Optional Daytona overrides (SDK defaults shown)
# DAYTONA_API_URL=https://app.daytona.io/api
# DAYTONA_TARGET=us

# Optional fallback auth if create() is called without auth:
OPENCODE_ZEN_API_KEY=your_opencode_zen_key
OPENCODE_PROVIDER=opencode
# Optional override for non-Zen providers in example CLI
OPENCODE_PROVIDER_API_KEY=

# Optional proxy mode keys
SERVERBOX_ADMIN_API_KEY=change_me
# Optional. If omitted, proxy routes reuse SERVERBOX_ADMIN_API_KEY.
# Set to an empty value only if you intentionally want to manage this in code.
SERVERBOX_PROXY_API_KEY=
SERVERBOX_PROXY_HOST=0.0.0.0
SERVERBOX_PROXY_PORT=7788
SERVERBOX_PROXY_URL=http://127.0.0.1:7788
SERVERBOX_LOG_LEVEL=debug
SERVERBOX_PROXY_REQUEST_LOGS=true
SERVERBOX_PROXY_AUTO_RESUME=true
SERVERBOX_PROXY_RESUME_TIMEOUT_MS=60000
SERVERBOX_PROXY_REQUEST_TIMEOUT_MS=60000
SERVERBOX_DB_PATH=./serverbox.db
SERVERBOX_INSTANCE_ID=
SERVERBOX_KEEP_INSTANCE=false
